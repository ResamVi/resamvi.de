server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;

	index index.html;

	server_name _;

	location / {
		try_files $uri $uri/ /index.html =404;
	}
}

server {
	root /var/www/html;

	index index.html;
	server_name resamvi.de; # managed by Certbot


	location / {
		try_files $uri $uri/ /index.html;
	}
	
	location /spayle {
		index spayle.html;
	}

    listen [::]:443 ssl ipv6only=on; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/resamvi.de-0002/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/resamvi.de-0002/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = resamvi.de) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


	listen 80 ;
	listen [::]:80 ;
    server_name resamvi.de;
    return 404; # managed by Certbot


}
